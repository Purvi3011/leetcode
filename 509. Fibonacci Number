class Solution {
    public int fib(int n) {
       if(n == 0)
       {
           return 0;
       }
        List<Integer> list = new ArrayList<>();
        list.add(0);
        list.add(1);
        int count = 1;
        while(count != n){
            list.add(list.get(list.size()-1) + list.get(list.size()-2));
            count++;
        }
        return list.get(list.size()-1);

    }
   
}
